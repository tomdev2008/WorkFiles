.lbox-wrap{ border:solid 2px #C4D0CE; padding:3px; background:#fff; position:relative;}
.lbox-wrap .lbox-titbar{ display:block; height:34px; line-height:34px; overflow:hidden; background:#FB9202; padding:0 15px; font-size:14px; font-weight:bold; color:#fff;}
.lbox-wrap .btn-close{ position:absolute; background:url(../images/btn_close.png) no-repeat; width:29px; height:29px; right:-8px; top:-8px; cursor:pointer;}
.lbox-wrap .lbox-cons{ display:block; border:solid 1px #FB9202; }
.lbox-wrap .cons1{padding:15px 20px 10px 20px;}
.lbox-wrap .cons1 .show-img1{ display:block; border:solid 1px #aaa; padding:6px 7px; position:relative;}
.lbox-wrap .cons1 .show-img1 img{ width:518px; height:278px;}
.lbox-wrap .cons1 .show-img1 .yp1{ position:absolute; background:url(../images/yp1.png) no-repeat; width:93px; height:57px;}
.lbox-wrap .cons1 .show-text1{border:solid 1px #aaa; display:block;border-top:none; overflow:hidden;zoom:1; height:134px; background:url(../images/bg_line1.gif) 0 0; padding:0 0 0 10px;}
.lbox-wrap .cons1 .show-text1 cite{ float:left; line-height:27px; font-size:14px; font-weight:bold; color:#666; margin-right:10px;}
.lbox-wrap .cons1 .show-text1 textarea{ margin:0; float:left; border:none; padding:0; background:none; height:134px; line-height:27px; width:467px; overflow-y:auto; font-size:12px; font-family:Verdana, Arial, "宋体";}
.lbox-wrap .cons1 .show-tips1{ display:block; height:26px; line-height:26px; background:url(../images/ico_xl.gif) no-repeat 0 4px; padding-left:20px;}

/*common*/
html, body, div, span, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, address, big, cite, code, del, em, font, img, ins, small, strong, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend {margin:0;padding:0;}
ul,li,dl,dd {list-style:none;}
:focus {outline:0;}
a img {border:0;}
a{ color:#333; text-decoration:none;}
a:hover{ color:#f60; text-decoration:underline;}
.orange{ color:#f60;}
img {vertical-align:middle;}
table {empty-cells:show;}
ins {text-decoration:none;}
del {text-decoration:line-through;}
em, i, cite {font-style:normal;}
b {font-weight:normal;}
body {font-family:SimSun;background: url(../images/bg.jpg) no-repeat center top #fff; font-size:12px; color:#333;}
.wrap{ width:966px; margin: 0 auto; background:url(../images/main_conbg.gif) repeat-y;}
.top-wrap{ background:url(../images/topbg.jpg) no-repeat; width:966px; height:90px; position:relative;}
.top-link{ position:absolute; right:50px; top:5px; color:#BA2612; line-height:30px;overflow:hidden;zoom:1;}
.top-link a{ float:left; color:#BA2612; margin:0 6px;}
.top-link a:hover{color:#BA2612;}
.top-link i{ float:left;}
.top-link a.wb_sina{ background:url(../images/ico_sina.png) no-repeat 0 4px; padding-left:24px;}
.menu{ background:url(../images/menubg.gif) no-repeat; width:966px; height:45px; overflow:hidden;zoom:1;}
.menu ul{ overflow:hidden;zoom:1; padding-left:80px; float:left;}
.menu ul li{ float:left; background:url(../images/menubg_line.gif) no-repeat right; height:45px; line-height:40px; font-size:14px; font-weight:bold; }
.menu ul li a{ display:block; height:45px;padding:0 24px 0 20px; float:left; color:#F8F5E3;}
.menu ul li a:hover{ color:#fff; }
.menu ul li.on{ background:url(../images/menubg_on.gif) repeat-x; color:#A10100; margin-left:-4px;}
.menu ul li.on a,.menu ul li.on a:hover{color:#A10100;text-decoration:none;}
.menu i{ background:url(../images/menubg.gif) no-repeat -30px 0; height:45px; width:10px; float:left; margin-left:-4px;}
.mainbox{ overflow:hidden;zoom:1; padding:6px 5px 10px 5px;}
.mainbox .m-left{ float:left; width:689px; overflow:hidden;}
.mainbox .m-right{ float:right; width:259px; overflow:hidden;}

.loginbox{ border:solid 1px #BEB8B3; background:url(../images/login_conbg.gif) 0 0; height:154px; padding:1px;}
.loginbox .ltitbar{ display:block; background:url(../images/login_titbg.gif) no-repeat; width:255px; height:39px; line-height:35px; overflow:hidden;zoom:1; color:#fff;}
.loginbox .ltitbar h3{ float:left; padding-left:45px; font-size:14px;}
.loginbox .ltitbar h3.dl{ font-weight:normal; font-size:12px;}
.loginbox .ltitbar a{ float:right; color:#fff; font-size:12px; margin-right:10px;}
.loginbox .ltitbar a:hover{ color:#fff;}
.loginbox ul{ padding:2px 10px;}
.loginbox ul li{ overflow:hidden;zoom:1; vertical-align:bottom; line-height:20px; padding:3px 0 2px 0;}
.loginbox ul li cite{ float:left; width:68px; text-align:right;}
.loginbox ul li em{ color:#f60; float:left; margin-left:5px;}
.loginbox ul li input{ float:left; width:80px; border:solid 1px #ABADB3; background:#fff; height:18px; line-height:18px; padding:0 5px; font-size:12px; font-family:Verdana, Arial, "宋体";}
.loginbox ul li input.inputstyle1{ width:130px;}
.loginbox ul li input.inputstyle2{ width:69px;}
.loginbox ul li img{ float:left; margin-left:5px;}
.loginbox ul li a{ float:left; margin-left:5px; white-space:nowrap;}
.loginbox ul li button{ background: url(../images/btns.gif) no-repeat -130px 0; width:81px; height:23px; border:none; float:left; margin:0; overflow:hidden; cursor:pointer;}
.loginbox ul li.lbtn{ padding-left:68px; padding-top:6px; line-height:24px;}
.loginbox .dl-btns p{ overflow:hidden;zoom:1;}
.loginbox .dl-btns p a{ float:left; background:url(../images/btns.gif) no-repeat; text-indent:-100em; overflow:hidden; height:38px; margin:12px 5px 0 5px;}
.loginbox .dl-btns p a.wdsc{ background-position:0 -29px; width:104px;}
.loginbox .dl-btns p a.wdfsjl{ background-position:-109px -29px; width:129px;}
.loginbox .dl-btns p a.wdlxr{ background-position:-243px -29px; width:104px;}

.block-titbar{ background:url(../images/titbar1.jpg) no-repeat; width:689px; height:32px; line-height:38px; overflow:hidden;zoom:1;}
.block2 .block-titbar{ background:url(../images/titbar2.jpg) no-repeat;}
.block3 .block-titbar{ background:url(../images/titbar3.jpg) no-repeat;}
.block4 .block-titbar{ background:url(../images/titbar4.jpg) no-repeat;}
.block5 .block-titbar{ background:url(../images/titbar5.jpg) no-repeat;}

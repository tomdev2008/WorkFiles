.ph-list li.num3{background:url(../images/n3.jpg) no-repeat 5px 12px;}
.ph-list li.num4{background:url(../images/n4.jpg) no-repeat 5px 12px;}
.ph-list li.num5{background:url(../images/n5.jpg) no-repeat 5px 12px;}
.ph-list li.num6{background:url(../images/n6.jpg) no-repeat 5px 12px;}
.ph-list li.num7{background:url(../images/n7.jpg) no-repeat 5px 12px;}
.ph-list li.num8{background:url(../images/n8.jpg) no-repeat 5px 12px;}
.ph-list li.num9{background:url(../images/n9.jpg) no-repeat 5px 12px;}

.foot{ background:url(../images/footbg.jpg) no-repeat 0 0; height:56px; width:966px; margin: 0 auto;}
.foot p{ text-align:center; line-height:22px; height:44px; overflow:hidden; font-family:Verdana, Arial, "宋体"; font-size:12px; color:#fff; padding-top:10px;}
.foot p a,.foot p a:hover{ color:#fff;}

/*弹窗*/
.lbox608{ width:608px; overflow:hidden;}
.lbox608 i{  height:11px; line-height:11px; font-size:1px; display:block;}
.lbox608 i.lbox-tbg{background:url(../images/lb_tbg_608.png) no-repeat;}
.lbox608 i.lbox-bbg{background:url(../images/lb_bbg_608.png) no-repeat;}
.lbox608 .lbox-bg{ background:url(../images/lb_bg_608.png) repeat-y; padding:0 11px;}
.lbox466{ width:466px; overflow:hidden;}

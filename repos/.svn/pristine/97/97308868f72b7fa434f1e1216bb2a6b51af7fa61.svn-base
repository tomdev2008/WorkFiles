.ph-list{ padding:0 5px 10px 5px;}
.ph-list li{ border-bottom:dashed 1px #ccc; height:24px; line-height:24px; padding:8px 5px 0 28px; vertical-align:bottom; position:relative;}
.ph-list li.num1{background:url(../images/n1.jpg) no-repeat 5px 12px;}

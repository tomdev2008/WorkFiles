.lbox-wrap .cons2 .lbox-l .lbox-sj cite{ float:left; padding:5px 0 0 8px;}
.lbox-wrap .cons2 .lbox-l .lbox-sj span{ float:left; background:url(../images/input_box1.gif) no-repeat; width:267px; height:40px; padding:1px;}
.lbox-wrap .cons2 .lbox-l .lbox-sj span textarea{ margin:0; float:left; border:none; padding:0 5px; background:none; height:40px; line-height:20px; width:257px; overflow-y:auto; font-size:12px; font-family:Verdana, Arial, "宋体";}
.lbox-wrap .cons2 .lbox-l .lbox-sjts{ width:326px; display:block; border:solid 1px #DEE3E6;background:#fff; overflow:hidden;zoom:1; padding:0 5px; line-height:24px; text-align:center; color:#999;}
.lbox-wrap .cons2 .lbox-l .show-img2{ width:326px; border:solid 3px #DEE3E6; background:#fff; display:block; padding:3px 2px 3px 4px; margin-top:10px; position:relative;}
.lbox-wrap .cons2 .lbox-l .show-img2 .yp2{ position:absolute; background:url(../images/yp2.png) no-repeat; width:53px; height:34px;}
.lbox-wrap .cons2 .lbox-l .show-img2 img{ width:324px; height:177px;vertical-align:bottom; margin:0 auto;}
.lbox-wrap .cons2 .lbox-l .show-img2 .show-text2{ display:block;background:url(../images/bg_line2.gif) 0 0; padding:0; height:53px; overflow:hidden; line-height:18px; margin:2px 8px;}
.lbox-wrap .cons2 .lbox-l .show-img2 .show-text2 cite{ float:left; font-weight:bold;}
.lbox-wrap .cons2 .lbox-l .show-img2 .show-text2 p{ float:left; width:270px;}
.lbox-wrap .cons2 .lbox-l .show-music{ display:block; margin-top:8px;overflow:hidden;zoom:1;}
.lbox-wrap .cons2 .lbox-l .show-music input{ float:left; margin-right:5px;}
.lbox-wrap .cons2 .lbox-l .show-music cite{ float:left;margin-right:5px; margin-top:3px;*margin-top:5px;}
.lbox-wrap .cons2 .lbox-l .show-music select{ float:left; font-size:12px; color:#666;}
.lbox-wrap .cons2 .lbox-l .show-time{ display:block; border:solid 1px #ECE1B3; margin-top:10px;}
.lbox-wrap .cons2 .lbox-l .show-time .show-time-titbar{ display:block; background:#F0DDB5; height:22px; line-height:22px;}
.lbox-wrap .cons2 .lbox-l .show-time .show-time-titbar input{ position:relative; top:3px;*top:1px;}
.lbox-wrap .cons2 .lbox-l .show-time .show-time-titbar em{ color:#999;}
.lbox-wrap .cons2 .lbox-l .show-time .show-time-cons{ display:block; overflow:hidden;zoom:1; padding:8px 5px; height:24px;}
.lbox-wrap .cons2 .lbox-l .show-time .show-time-cons select{ float:left; width:40px; margin-right:3px; font-size:12px; font-family:Verdana;}
.lbox-wrap .cons2 .lbox-l .show-time .show-time-cons em{ float:left; padding-top:3px;*padding-top:5px;margin-right:5px;}
.lbox-wrap .cons2 .lbox-l .show-time p{ text-align:center; padding:0 0 5px 0; color:#f60;}
.lbox-wrap .cons2 .lbox-r{ float:right; width:212px;overflow:hidden; border:solid 1px #B3B3B3; height:448px; background:#F5F5F3;}
.lbox-wrap .cons2 .lbox-r .sou{ display:block; background:url(../images/input_box2.gif) no-repeat; width:203px; height:27px; margin:5px; overflow:hidden;zoom:1;}
.lbox-wrap .cons2 .lbox-r .sou input{ float:left; border:none; background:none; padding:0 5px; height:27px; line-height:27px; width:160px; font-size:12px; font-family:Verdana, Arial, "宋体";}
.lbox-wrap .cons2 .lbox-r .sou button{ float:left; width:25px; height:25px; background:none; border:none; cursor:pointer;}
.lbox-wrap .cons2 .lbox-r .sou-list-tit{ display:block; border-top:solid 1px #B3B3B3; background:#CCD7E9; height:24px; line-height:24px; padding-left:10px; font-weight:bold;}
.lbox-wrap .cons2 .lbox-r ul{ background:#fff; height:386px; overflow-y:scroll;}
.lbox-wrap .cons2 .lbox-r ul li{ background:url(../images/ico_jt.gif) no-repeat 10px 6px; height:26px; line-height:26px; padding:0 10px 0 25px; overflow:hidden; cursor:pointer;}
.lbox-wrap .cons2 .lbox-r ul li.li_over{ background-color:#F7F7F7;}
.alert-tips{ background:url(../images/ico_yes.gif) no-repeat 30px 20px; height:160px; display:block;}
.alert-tips p{ font-size:14px; font-weight:bold; color:#f60; padding:60px 30px 0 150px; line-height:26px;}
.t-yes{ background:url(../images/ico_yes.gif) no-repeat 30px 20px;}
.t-no{ background:url(../images/ico_no.gif) no-repeat 30px 20px;}
.lbox-wrap .lbox-btns{ display:block; text-align:center; padding:10px 0;}

/*按钮*/
.lbox-btns button{ background:url(../images/btns.gif) no-repeat; border:none; cursor:pointer; margin:0 3px;}
.lbox-btns button.btns-next{ width:95px; height:36px; background-position:-100px -113px;}
.lbox-btns button.btns-send{ width:95px; height:36px; background-position:0 -113px;}
.lbox-btns button.btns-again{ width:95px; height:36px; background-position:0 -72px;}
.lbox-btns button.btns-cancel{ width:85px; height:36px; background-position:-280px -72px;}
.lbox-btns button.btns-modify{ width:85px; height:36px; background-position:-190px -72px;}
.lbox-btns button.btns-close{ width:85px; height:36px; background-position:-100px -72px;}







